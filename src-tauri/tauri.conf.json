{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "devPath": "http://localhost:4242",
        "distDir": "../out"
    },
    "package": {
        "productName": "keebox",
        "version": "0.1.0"
    },
    "tauri": {
        "windows": [
            {
                "url": "splashscreen.html",
                "label": "splashscreen",
                "width": 500,
                "height": 300,
                "center": true,
                "visible": true,
                "resizable": false,
                "fullscreen": false,
                "decorations": false
            },
            {
                "title": "Keebox",
                "width": 800,
                "height": 600,
                "center": true,
                "visible": false,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "ShyAssassin",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": ["icons/32x32.png", "icons/64x64.png", "icons/128x128.png", "icons/512x512.png", "icons/icon.icns", "icons/icon.ico"],
            "identifier": "com.shyassassin.keebox",
            "longDescription": "A all in one keyboard testing and managment tool",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "allowlist": {
            "all": false
        },
        "updater": {
            "active": false
        },
        "security": {
            "csp": null
        }
    }
}
